ORG 0000H
AJMP LEAVE_2    
ORG 0030H

MAIN:
    MOV A,#11111110B
    MOV P1,A 
    AJMP LOOP
LOOP:
    RL A 
    MOV P1,A 
    ACALL DELAY
    AJMP LOOP

;A F B C D E ;0 5 1 2 3 4
LEAVE_1:
    MOV P2,#00000001B;LED1 0
    MOV P0,#11000000B
    ACALL DELAY
    MOV P2,#00000010B 
    MOV P0,#11111001B
    ACALL DELAY
    MOV P2,#00000100B
    MOV P0,#10100100B
    ACALL DELAY
    MOV P2,#00001000B 
    MOV P0,#10110000B
    ACALL DELAY
    MOV P2,#00010000B 
    MOV P0,#10011001B
    ACALL DELAY 
    MOV P2,#00100000B 
    MOV P0,#10010010B
    ACALL DELAY
    AJMP LEAVE_1

LEAVE_2:
    MOV P3,#11110000B
    AJMP LEAVE_2


DELAY:
    D0:
        MOV R0,#0FFH
    D1:
        MOV R1,#0FFH
    D2:
        DJNZ R1,D2
        DJNZ R0,D1
        RET
        END